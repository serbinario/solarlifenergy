	<queryString language="SQL">
		<![CDATA[SELECT 
cl.nome, 
cl.cpf_cnpj,  
cl.endereco, 
p.kw,
p.end_intalacao,
p.cep,
p.cidade,
p.estado,
c.ano 
FROM  
contratos AS c
INNER JOIN projetos AS p ON p.id = c.projeto_id
INNER JOIN clientes AS cl ON cl.id = p.clientes_id
where c.id =  $P{id}]]>
