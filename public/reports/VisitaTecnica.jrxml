	<parameter name="id" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[SELECT 
users.name AS responsavel,
clientes.nome AS cliente,
pre_propostas.codigo AS NumeroProposta,
ende.logradouro,
ende.cep,
ende.numero,
ende.complemento,
ende.bairro,
ende.uf,
ende.cidade,
        ie_fibrocimento,
        ie_metalico,
        ie_barro,
        ie_fibrocim_alta,
        ie_laje,
        ie_outros,
        ie_area_suficiente,
        ie_estrutura_estar_apta,
        ie_reforcos_necessarios,
        ie_ha_vazamentos,
        ie_altura,
        qtd_materiais_reforco,
        riscos_acidentes,
        material_especifico,
        distancia_circuito_placas,
        distancia_circuito_inversor_quadro_geral,
        pe_metalico,
        pe_barro,
        pe_fibrocim_alta,
        pe_dijuntor_geral,
        pe_tampa_medidor,
        pe_caixa_medidor,
        pe_caixa_disjuntor,
        pe_bitola_cabo_medidor_disjuntor_geral,
        pe_bitola_rede_publica,
        vt.obs,
        tecnico_id,
        status_visita_id,
        data_previsao,
        data_visita 
FROM visita_tecnica AS vt
LEFT JOIN projetosv2 ON projetosv2.id = vt.projeto_id
LEFT JOIN enderecos AS ende ON ende.id = projetosv2.endereco_id
LEFT JOIN pre_propostas ON pre_propostas.id = projetosv2.proposta_id
LEFT JOIN clientes ON clientes.id = pre_propostas.cliente_id
LEFT JOIN users ON  users.id = vt.tecnico_id
WHERE vt.id =   $P{id}]]>
	</queryString>
